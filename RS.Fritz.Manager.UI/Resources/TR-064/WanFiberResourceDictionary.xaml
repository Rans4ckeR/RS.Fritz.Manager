<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:api="clr-namespace:RS.Fritz.Manager.API;assembly=RS.Fritz.Manager.API">
    <DataTemplate DataType="{x:Type api:WanFiberGetInfoResponse}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Label Grid.Row="0" Grid.Column="0" Target="{Binding ElementName=OpticalSignalLevel}" Content="_OpticalSignalLevel" ToolTip="Current measurement of total downstream optical signal level in dBm/1000."/>
            <Label Grid.Row="1" Grid.Column="0" Target="{Binding ElementName=LowerOpticalThreshold}" Content="_LowerOpticalThreshold" ToolTip="Optical level that is used to declare the downstream low received optical power alarm in dBm/1000."/>
            <Label Grid.Row="2" Grid.Column="0" Target="{Binding ElementName=UpperOpticalThreshold}" Content="_UpperOpticalThreshold" ToolTip="Optical level that is used to declare the downstream high received optical power alarm in dBm/1000."/>
            <Label Grid.Row="3" Grid.Column="0" Target="{Binding ElementName=TransmitOpticalLevel}" Content="_TransmitOpticalLevel" ToolTip="Current measurement of mean optical launch power in dBm/1000."/>
            <Label Grid.Row="4" Grid.Column="0" Target="{Binding ElementName=LowerTransmitPowerThreshold}" Content="_LowerTransmitPowerThreshold" ToolTip="Minimum mean optical launch power that is used to declare the low transmit optical power alarm in dBm/1000."/>
            <Label Grid.Row="5" Grid.Column="0" Target="{Binding ElementName=UpperTransmitPowerThreshold}" Content="_UpperTransmitPowerThreshold" ToolTip="Maximum mean optical launch power that is used to declare the high transmit optical power alarm in dBm/1000."/>
            <Label Grid.Row="6" Grid.Column="0" Target="{Binding ElementName=SfpVendor}" Content="_SfpVendor" ToolTip="The SFP vendor."/>
            <Label Grid.Row="7" Grid.Column="0" Target="{Binding ElementName=SfpPartNumber}" Content="_SfpPartNumber" ToolTip="The SFP part number."/>
            <Label Grid.Row="8" Grid.Column="0" Target="{Binding ElementName=SfpSerialNumber}" Content="_SfpSerialNumber" ToolTip="The SFP serial number."/>
            <Label Grid.Row="9" Grid.Column="0" Target="{Binding ElementName=SfpType}" Content="_SfpType" ToolTip="The SFP type."/>
            <Label Grid.Row="10" Grid.Column="0" Target="{Binding ElementName=TxWaveLength}" Content="_TxWaveLength" ToolTip="The transmit wave length."/>
            <Label Grid.Row="11" Grid.Column="0" Target="{Binding ElementName=FiberMode}" Content="_FiberMode" ToolTip="Current Fiber mode. Possible values are AON, GPON, XGPON, XGSPON, 1000BASE_BX10, 10GBASE_BX, 1000BASE_T, 2.5GBASE_T, 10GBASE_T, Init, Unknown."/>
            <TextBlock x:Name="OpticalSignalLevel" Grid.Row="0" Grid.Column="1" Text="{Binding OpticalSignalLevel}"/>
            <TextBlock x:Name="LowerOpticalThreshold" Grid.Row="1" Grid.Column="1" Text="{Binding LowerOpticalThreshold}"/>
            <TextBlock x:Name="UpperOpticalThreshold" Grid.Row="2" Grid.Column="1" Text="{Binding UpperOpticalThreshold}"/>
            <TextBlock x:Name="TransmitOpticalLevel" Grid.Row="3" Grid.Column="1" Text="{Binding TransmitOpticalLevel}"/>
            <TextBlock x:Name="LowerTransmitPowerThreshold" Grid.Row="4" Grid.Column="1" Text="{Binding LowerTransmitPowerThreshold}"/>
            <TextBlock x:Name="UpperTransmitPowerThreshold" Grid.Row="5" Grid.Column="1" Text="{Binding UpperTransmitPowerThreshold}"/>
            <TextBlock x:Name="SfpVendor" Grid.Row="6" Grid.Column="1" Text="{Binding SfpVendor}"/>
            <TextBlock x:Name="SfpPartNumber" Grid.Row="7" Grid.Column="1" Text="{Binding SfpPartNumber}"/>
            <TextBlock x:Name="SfpSerialNumber" Grid.Row="8" Grid.Column="1" Text="{Binding SfpSerialNumber}"/>
            <TextBlock x:Name="SfpType" Grid.Row="9" Grid.Column="1" Text="{Binding SfpType}"/>
            <TextBlock x:Name="TxWaveLength" Grid.Row="10" Grid.Column="1" Text="{Binding TxWaveLength}"/>
            <TextBlock x:Name="FiberMode" Grid.Row="11" Grid.Column="1" Text="{Binding FiberMode}"/>
        </Grid>
    </DataTemplate>
    <DataTemplate DataType="{x:Type api:WanFiberGetInfoGponResponse}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Label Grid.Row="0" Grid.Column="0" Target="{Binding ElementName=GponSerial}" Content="_GponSerial" ToolTip="The Gigabit Passive Optical Networks serial."/>
            <Label Grid.Row="1" Grid.Column="0" Target="{Binding ElementName=PonId}" Content="_PonId" ToolTip="The Passive Optical Networks id."/>
            <Label Grid.Row="2" Grid.Column="0" Target="{Binding ElementName=OnuId}" Content="_OnuId" ToolTip="The Optical Network Unit id."/>
            <Label Grid.Row="3" Grid.Column="0" Target="{Binding ElementName=UniType}" Content="_UniType" ToolTip="The User Network Interface type. Possible values are PPTP, VEIP."/>
            <Label Grid.Row="4" Grid.Column="0" Target="{Binding ElementName=GemPortCount}" Content="_GemPortCount" ToolTip="The GPON Encapsulation Method port count."/>
            <TextBlock x:Name="GponSerial" Grid.Row="0" Grid.Column="1" Text="{Binding GponSerial}"/>
            <TextBlock x:Name="PonId" Grid.Row="1" Grid.Column="1" Text="{Binding PonId}"/>
            <TextBlock x:Name="OnuId" Grid.Row="2" Grid.Column="1" Text="{Binding OnuId}"/>
            <TextBlock x:Name="UniType" Grid.Row="3" Grid.Column="1" Text="{Binding UniType}"/>
            <TextBlock x:Name="GemPortCount" Grid.Row="4" Grid.Column="1" Text="{Binding GemPortCount}"/>
        </Grid>
    </DataTemplate>
    <DataTemplate DataType="{x:Type api:WanFiberGetStatisticsResponse}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Label Grid.Row="0" Grid.Column="0" Target="{Binding ElementName=BytesSent}" Content="_BytesSent"/>
            <Label Grid.Row="1" Grid.Column="0" Target="{Binding ElementName=BytesReceived}" Content="_BytesReceived"/>
            <Label Grid.Row="2" Grid.Column="0" Target="{Binding ElementName=PacketsSent}" Content="_PacketsSent"/>
            <Label Grid.Row="3" Grid.Column="0" Target="{Binding ElementName=PacketsReceived}" Content="_PacketsReceived"/>
            <Label Grid.Row="4" Grid.Column="0" Target="{Binding ElementName=PacketErrorsSent}" Content="_PacketErrorsSent"/>
            <Label Grid.Row="5" Grid.Column="0" Target="{Binding ElementName=PacketErrorsReceived}" Content="_PacketErrorsReceived"/>
            <Label Grid.Row="6" Grid.Column="0" Target="{Binding ElementName=PacketsMulticast}" Content="_PacketsMulticast"/>
            <Label Grid.Row="7" Grid.Column="0" Target="{Binding ElementName=ConnectionRateDown}" Content="_ConnectionRateDown"/>
            <Label Grid.Row="8" Grid.Column="0" Target="{Binding ElementName=ConnectionRateUp}" Content="_ConnectionRateUp"/>
            <Label Grid.Row="9" Grid.Column="0" Target="{Binding ElementName=BestTrainState}" Content="_BestTrainState"/>
            <Label Grid.Row="10" Grid.Column="0" Target="{Binding ElementName=Resyncs}" Content="_Resyncs"/>
            <Label Grid.Row="11" Grid.Column="0" Target="{Binding ElementName=MinutesInShowtime}" Content="_MinutesInShowtime"/>
            <TextBlock x:Name="BytesSent" Grid.Row="0" Grid.Column="1" Text="{Binding BytesSent}"/>
            <TextBlock x:Name="BytesReceived" Grid.Row="1" Grid.Column="1" Text="{Binding BytesReceived}"/>
            <TextBlock x:Name="PacketsSent" Grid.Row="2" Grid.Column="1" Text="{Binding PacketsSent}"/>
            <TextBlock x:Name="PacketsReceived" Grid.Row="3" Grid.Column="1" Text="{Binding PacketsReceived}"/>
            <TextBlock x:Name="PacketErrorsSent" Grid.Row="4" Grid.Column="1" Text="{Binding PacketErrorsSent}"/>
            <TextBlock x:Name="PacketErrorsReceived" Grid.Row="5" Grid.Column="1" Text="{Binding PacketErrorsReceived}"/>
            <TextBlock x:Name="PacketsMulticast" Grid.Row="6" Grid.Column="1" Text="{Binding PacketsMulticast}"/>
            <TextBlock x:Name="ConnectionRateDown" Grid.Row="7" Grid.Column="1" Text="{Binding ConnectionRateDown}"/>
            <TextBlock x:Name="ConnectionRateUp" Grid.Row="8" Grid.Column="1" Text="{Binding ConnectionRateUp}"/>
            <TextBlock x:Name="BestTrainState" Grid.Row="9" Grid.Column="1" Text="{Binding BestTrainState}"/>
            <TextBlock x:Name="Resyncs" Grid.Row="10" Grid.Column="1" Text="{Binding Resyncs}"/>
            <TextBlock x:Name="MinutesInShowtime" Grid.Row="11" Grid.Column="1" Text="{Binding MinutesInShowtime}"/>
        </Grid>
    </DataTemplate>
</ResourceDictionary>